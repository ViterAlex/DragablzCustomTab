<Window x:Class="DragablzCustomTab.TabHostWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
        xmlns:local="clr-namespace:DragablzCustomTab"
        mc:Ignorable="d"
        Title="TabHostWindow" Height="300" Width="300">
    <Grid>
        <dragablz:TabablzControl Name="TabsContainer">
            <dragablz:TabablzControl.InterTabController>
                <dragablz:InterTabController></dragablz:InterTabController>
            </dragablz:TabablzControl.InterTabController>
        </dragablz:TabablzControl>
    </Grid>
</Window>
